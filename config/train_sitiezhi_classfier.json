{
    "dataset": {
        "repo_id": "local/test_hil_serl_v0.5_cropped_v2",
        "root": "/shared_disk/users/qiuping.deng/code/lerobot/dataset/test_hil_serl_v0.5_cropped_v2"
    },
    "policy": {
        "type": "reward_classifier",
        "repo_id": "local/reward_classifier_v1",
        "model_name": "microsoft/resnet-18",
        "num_cameras": 2,
        "num_classes": 2,
        "hidden_dim": 256,
        "dropout_rate": 0.1,
        "learning_rate": 1e-4,
        "device": "cuda",
        "use_amp": true,
        "normalization_mapping": {
            "VISUAL": "MEAN_STD",
            "STATE": "MIN_MAX",
            "ACTION": "MIN_MAX"
        },
        "input_features": {
            "observation.images.cam_high": {
                "type": "VISUAL",
                "shape": [3, 128, 128]
            },
            "observation.images.cam_left_wrist": {
                "type": "VISUAL",
                "shape": [3, 128, 128]
            },
            "observation.state": {
                "type": "STATE",
                "shape": [28]
            },
            "action": {
                "type": "ACTION",
                "shape": [14]
            }
        }
    },
    "output_dir": "outputs/train/test_hil_serl_v0.5_cropped_v2",
    "batch_size": 16,
    "num_workers": 4,
    "steps": 20000,
    "log_freq": 10,
    "eval_freq": 1000,
    "save_freq": 2000,
    "save_checkpoint": true,
    "seed": 2,
    "resume": false,
    "optimizer": {
        "type": "adamw",
        "lr": 0.0001,
        "weight_decay": 0.01,
        "grad_clip_norm": 1.0
    },
    "wandb": {
        "enable": false,
        "project": "reward-classifier",
        "disable_artifact": false
    },
    "job_name": "reward-classifier"
}